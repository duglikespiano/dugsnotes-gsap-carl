  <script src="https://cdn.jsdelivr.net/npm/gsap@3.13.0/dist/ScrollToPlugin.min.js"></script>
  <script>
    gsap.registerPlugin(GSDevTools, ScrollTrigger, SplitText, TextPlugin, ScrollToPlugin)
